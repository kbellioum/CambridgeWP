<% include header.ejs %>
<!-- page content -->
<div class="right_col" role="main">
  <div class="">
    <div class="row">

        <div class="x_panel">

          <div class="x_content">





            <form name="myForm" class="form-horizontal form-label-left input_mask" validate action="/stockout/<%= stockin._id %>" method="post"  onsubmit="return(validate());">
             <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Nom du Dépôt : <span class="required">*</span>
                </label>
                <div class="col-md-6 col-sm-6 col-xs-12">
                  <input disabled="disabled" class="form-control col-md-7 col-xs-12"  data-validate-length-range="6" data-validate-words="1" name="depotname" placeholder="Nom du Dépôt" required="required" type="text" value="<%= stockin.depotname %>">
                </div>
              </div>
 
              <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Produit : <span class="required">*</span>
                </label>
                <div class="col-md-6 col-sm-6 col-xs-12">
                  <input disabled="disabled" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" data-validate-words="1" name="prodinfo" placeholder="Nom du produit" required="required" type="text" value="<%= stockin.prodcode + ' | ' + stockin.prodname %>">
                </div>
              </div>

              <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Stock initial : <span class="required">*</span>
                </label>
                <div class="col-md-6 col-sm-6 col-xs-12">
                  <input disabled="disabled" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" data-validate-words="1" name="prodqteinit" placeholder="Stock initial" required="required" type="text" value="<%= stockin.prodqteinit %> <%= stockin.produnite %>">
                </div>
              </div>

              <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Stock actuel ( en unité) : <span class="required">*</span>
                </label>
                <div disabled="disabled" class="col-md-6 col-sm-6 col-xs-12">
                  <input disabled="disabled" class="form-control col-md-7 col-xs-12" data-validate-length-range="6" data-validate-words="1" id="prodqtemvt" name="prodqtemvt" placeholder="Stock actuel" required="required" type="text" value="<%= stockin.prodqtemv %>">
                </div>
              </div>


             <div class="clearfix"></div>
             <div class="ln_solid"></div>

             <div class="form-group">
          		 <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">Numéro Facture Client : <span class="required">*</span>
          		 </label>
          		 <div class="col-md-6 col-sm-6 col-xs-12">
          			 <select type="text" id="factureclt" name="factureclt"  data-validate-length-range="5,20" class="select2_single form-control col-md-7 col-xs-12" placeholder="Facture client" required="required" >
                   <option></option>

                   <% for(var i=0; i < patient.length; i++) { %>
                     <% for(j=0; j < patient[i].visites.length; j++){%>
                         <% if (patient[i].visites[j].factnum != undefined) { %>
                        <option ><%= patient[i].visites[j].factnum + ' | ' +  patient[i].patientnom + ' ' + patient[i].patientprenom  %> </option>
                  <%}}}%>

                 </select>
          		 </div>
          	 </div>




              <div class="form-group">
                <label class="control-label col-md-3 col-sm-3 col-xs-12">Quantité à sortir : <span class="required">*</span>
                </label>
                <div class="col-md-6 col-sm-6 col-xs-12">
                  <input class="form-control col-md-7 col-xs-12"  data-validate-words="1" id="qteout" name="qteout" placeholder="Quantité à sortir" autofocus required="required" type="Number" min="1" max="<%= stockin.prodqtemv %>">
                  <input class="form-control col-md-7 col-xs-12 hidden"  data-validate-words="1" id="prodid2" name="prodid2" placeholder="idprod" autofocus required="required" type="text" value="<%= stockin.prodid %>">
                </div>
              </div>

           </div>
           <div class="col-md-12 col-sm-12 col-xs-12">
             <div class="form-group" align="right">
               <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                 <input type="hidden" name="_method" value="PUT">
                 <button id="send" type="submit" class="btn btn-success" >Valider</button>
                 <button id="retour" type="button" class="btn btn-success" onclick="window.location.href='/listinout/<%= stockin.prodid %>'">Liste Etat Stock</button>
               </div>
             </div>
           </div
          </form>

        </div>
      </div>
    </div>
<!-- this -->

<!-- end this -->


  </div>
</div>
<!-- /page content -->

<!-- /page content -->
<% include footer.ejs %>

